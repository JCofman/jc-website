  "main": "n/a",
  "scripts": {
    "build": "PUBLISH_ON_NOW=branch-deploy gatsby build",
    "build:prod": "PUBLISH_ON_NOW=production gatsby build",
    "develop": "npm-run-all -p dev storybook",
    "serve": "gatsby serve",
    "dev": "PUBLISH_ON_NOW=branch-deploy gatsby develop",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "format": "npm run prettier --write 'src/**/*.js'",
    "check-format": "npm run prettier  -- --list-different",
    "prettier": "prettier --ignore-path .gitignore --write 'src/**/*.js'",
    "start": "npm-run-all -p dev storybook",
    "start:ci": "serve --no-clipboard --listen 8899",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watch",
    "test:size": "npm-run-all -p test:size:bundlesize",
    "test:size:bundlesize": "bundlesize",
    "test:e2e:ci": "start-server-and-test start:ci http://localhost:8000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:8000 cy:dev",
    "cy:run": "cypress run",
    "cy:dev": "cypress open",
    "deploy": "now ./public --token=$NOW_TOKEN --target staging --local-config=../now.json",
    "deploy:prod": "now ./public --token=$NOW_TOKEN --prod --local-config=../now.json",
    "build-deploy-and-alias-prod": "npm-run-all -s build:prod deploy:prod",
    "lighthouse": "lighthouse-ci https://jcofman.now.sh --score=90 --performance=85 --pwa=90 --accessibility=90 --best-practice=90 --seo=90",
    "storybook": "NODE_ENV=production start-storybook -p 6006",
    "build:storybook": "NODE_ENV=production build-storybook",
    "deploy:storybook": "now ./storybook-static --token=$NOW_TOKEN --prod --local-config=../now.storybook.json",
    "build-deploy-and-alias-storybook": "npm-run-all -s build:storybook deploy:storybook",
    "clean": "gatsby clean",
    "validate": "npm-run-all --parallel lint test test:size:bundlesize"
  },
  "bundlesize": [
    {
      "path": "./public/component---src-pages*.js",
      "maxSize": "150 kB"
    },
    {
      "path": "./public/app*.js",
      "maxSize": "100 kB"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
